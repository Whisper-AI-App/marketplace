{
	"name": "whisper-llm-cards",
	"version": "1.8.0",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./cards.json": "./dist/cards.json"
	},
	"files": [
		"dist",
		"src",
		"scripts",
		"tsconfig.json",
		"types.d.ts",
		"cards.json"
	],
	"scripts": {
		"build": "tsx scripts/generate-version.ts && tsx scripts/generate-json.ts && tsc && tsx scripts/build.ts",
		"postinstall": "npm run build || pnpm build || yarn build || true",
		"lint": "biome lint",
		"format": "biome check --write",
		"test": "ava"
	},
	"ava": {
		"extensions": {
			"ts": "module"
		},
		"nodeArguments": [
			"--import=tsx",
			"--no-warnings"
		],
		"files": [
			"test/**/*.test.ts"
		],
		"workerThreads": false
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/Ava-Technologies-Org/whisper-llm-cards"
	},
	"packageManager": "pnpm@9.15.9",
	"dependencies": {
		"tsx": "^4.20.6",
		"typescript": "^5.7.3"
	},
	"devDependencies": {
		"@ava/typescript": "^6.0.0",
		"@biomejs/biome": "2.3.4",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/commit-analyzer": "^13.0.0",
		"@semantic-release/exec": "^6.0.3",
		"@semantic-release/git": "^10.0.1",
		"@semantic-release/github": "^11.0.0",
		"@semantic-release/npm": "^12.0.1",
		"@semantic-release/release-notes-generator": "^14.0.1",
		"@types/node": "^22.10.2",
		"ava": "^6.4.1",
		"conventional-changelog-conventionalcommits": "^8.0.0",
		"semantic-release": "^24.2.0"
	}
}
