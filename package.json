{
	"name": "whisper-llm-cards",
	"version": "1.0.0",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./cards.json": "./dist/cards.json"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsx scripts/generate-json.ts && tsc && tsx scripts/build.ts",
		"lint": "biome lint",
		"format": "biome check --write",
		"test": "ava"
	},
	"ava": {
		"extensions": {
			"ts": "module"
		},
		"nodeArguments": [
			"--import=tsx",
			"--no-warnings"
		],
		"files": [
			"test/**/*.test.ts"
		],
		"workerThreads": false
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/Ava-Technologies-Org/whisper-models"
	},
	"packageManager": "pnpm@9.15.9",
	"devDependencies": {
		"@ava/typescript": "^6.0.0",
		"@biomejs/biome": "2.3.4",
		"@types/node": "^22.10.2",
		"ava": "^6.4.1",
		"tsx": "^4.20.6",
		"typescript": "^5.7.3"
	}
}
